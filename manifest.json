{
  "manifest_version":2,    
  "name": "Tab Timer",
  "version": "1.0",
  "description": "Open tabs at a set time.",
  "icons": {"128": "clock.png"},    
  "browser_action": {
    "default_icon": "clock.png",
    "default_popup": "options.html" 
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
      "tabs", "alarms", "storage"
  ],
  "options_page":  "options.html",
  "options_ui": {
   /"page":  "options.html",
    "chrome_style": true
  }
       
}